{
  "name": "autonomous-mcp-system",
  "version": "1.0.0",
  "description": "Autonomous MCP/RAG Multi-Agent Development System - A self-sustaining 3-agent system using Model Context Protocol with RAG memory",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "setup": "tsx scripts/setup.ts",
    "start": "tsx scripts/launch-system.ts",
    "start:auditor": "cd worktrees/agent-auditor && claude --config ../../agents/auditor/config.json",
    "start:implementer": "cd worktrees/agent-implementer && claude --config ../../agents/implementer/config.json",
    "start:validator": "cd worktrees/agent-validator && claude --config ../../agents/validator/config.json",
    "monitor": "tsx src/coordination/monitor.ts",
    "debug": "tsx src/coordination/debug.ts",
    "recover": "tsx src/coordination/recovery-system.ts",
    "test": "tsx scripts/test-system.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "rag",
    "multi-agent",
    "autonomous-development",
    "claude",
    "ai-agents"
  ],
  "author": "MCP-RAG Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.3",
    "@zilliz/milvus2-sdk-node": "^2.5.11",
    "chokidar": "^3.6.0",
    "dotenv": "^16.4.5",
    "simple-git": "^3.21.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
